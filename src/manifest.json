{
  "name": "__MSG_extName__",
  "version": "0.1",
  "manifest_version": 2,
  "description": "__MSG_extDesc__",
  "icons": {
    "16": "images/icons/black_white/icon_016.png",
    "48": "images/icons/black_white/icon_048.png",
    "128": "images/icons/black_white/icon_128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "images/debuggerPause.png",
    "default_title": "__MSG_ext_default_title__"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "libs/async.js",
      "libs/socket.io.js",
      "scripts/background.js"
    ]
  },
  "homepage_url": "https://github.com/kevinvandervlist/asbru-chrome-plugin",
  "permissions": [
    "tabs",
    "debugger",
		"http://*/",
		"https://*/"
  ]
}
